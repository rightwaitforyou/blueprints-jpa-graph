<?xml version="1.0" encoding="UTF-8" ?>
  
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm orm_2_0.xsd"
                 version="2.0">

	 <entity class="com.wingnest.blueprints.impls.jpa.internal.models.BpJpaElement" access="FIELD">
		<table name="ELEMENT"></table>
		<attributes>
			<one-to-many name="propMap" orphan-removal="true" fetch="EAGER">
				<map-key name="keyName"/>				
				<join-column name="ELEMENT_ID"/>
				<cascade>
					<cascade-persist/>
					<cascade-remove/>
				</cascade>				
			</one-to-many>
		</attributes>
	</entity>
</entity-mappings>
